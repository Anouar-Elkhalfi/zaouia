


<%= simple_form_for([:admin, @medical_care]) do |f| %>
  <%= f.input :name %>
  <%= f.input :address %>
  <%= f.input :rating %>
  <%= f.input :phone %>

  <!-- Nouveaux champs -->
  <%= f.input :specialty %>
  <%= f.input :tarif %>
  <%= f.input :payment_methods %>
  <%= f.input :services %>
  <%= f.input :presentation %>
  <%= f.input :qualifications %>
  <%= f.input :hours %>

  <%= f.submit 'Save Medical Care', class: "btn btn-primary" %>
<% end %>
